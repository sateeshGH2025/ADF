{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFSateesh1"
		},
		"ADLS_Gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_Gen2'"
		},
		"ADLS_Gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devstgaccntcopy.dfs.core.windows.net/"
		},
		"DS_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://devkeyvaultsat.vault.azure.net/"
		},
		"LS_azuresqldb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "devsqlserverazure.database.windows.net"
		},
		"LS_azuresqldb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "devdatabasesateesh"
		},
		"LS_azuresqldb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sateesh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFSateesh1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_Gen2_accountKey": {
						"value": "[parameters('ADLS_Gen2_accountKey')]"
					},
					"ADLS_Gen2_properties_typeProperties_url": {
						"value": "[parameters('ADLS_Gen2_properties_typeProperties_url')]"
					},
					"DS_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('DS_AzureKeyVault_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFSateesh1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_azuresqldb_properties_typeProperties_server": {
						"value": "[parameters('LS_azuresqldb_properties_typeProperties_server')]"
					},
					"LS_azuresqldb_properties_typeProperties_database": {
						"value": "[parameters('LS_azuresqldb_properties_typeProperties_database')]"
					},
					"LS_azuresqldb_properties_typeProperties_userName": {
						"value": "[parameters('LS_azuresqldb_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFSateesh1_ArmTemplate_0')]"
			]
		},
		{
			"name": "ADFSateesh1_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFSateesh1_ArmTemplate_1')]"
			]
		},
		{
			"name": "ADFSateesh1_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
